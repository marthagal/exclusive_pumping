chisq.test(table(data$Difference_Category,data$Fenugreek))
chisq.test(table(data$Difference_Category,data$Oatmeal))
chisq.test(table(data$Difference_Category,data$Water))
chisq.test(table(data$Difference_Category,data$Power_Pumping))
chisq.test(table(data$Difference_Category,data$Beer))
chisq.test(table(data$Difference_Category,data$Blessed))
chisq.test(table(data$Difference_Category,data$Teas))
chisq.test(table(data$Difference_Category,data$Dom))
chisq.test(table(data$Difference_Category,data$Reglan))
chisq.test(table(data$Difference_Category,data$Why_EP_Physical))
chisq.test(table(data$Difference_Category,data$Why_EP_Refused_Latch))
chisq.test(table(data$Difference_Category,data$Why_EP_NICU))
chisq.test(table(data$Difference_Category,data$Why_EP_No_Weight_Gain))
chisq.test(table(data$Difference_Category,data$Why_EP_Painful))
chisq.test(table(data$Difference_Category,data$Why_EP_Biting))
chisq.test(table(data$Difference_Category,data$Health_Clogged))
chisq.test(table(data$Difference_Category,data$Health_Mastitis))
chisq.test(table(data$Difference_Category,data$Health_Thrush))
chisq.test(table(data$Difference_Category,data$Health_Galactoceles))
chisq.test(table(data$Difference_Category,data$Health_Cracked_Bleeding))
chisq.test(table(data$Difference_Category,data$Health_General_Pain))
chisq.test(table(data$Difference_Category,data$Health_No_Issues))
chisq.test(table(data$Difference_Category,data$Partner_Very_Supportive))
chisq.test(table(data$Difference_Category,data$Pump_Baby_Again)) #significant
chisq.test(table(data$Difference_Category,data$Age))
chisq.test(table(data$Difference_Category,data$Birth_Experience))
chisq.test(table(data$Difference_Category,data$Birth_Type))

