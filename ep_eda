# libraries
library(ggplot2) 
library(vcd) 

# Read data
data <- read.csv("cleaned_EP_data.csv")

# Continuous variables
continuous <- data[c(3:7)]

## Start Continuous- Continuous variable statisitics

means <- sapply(continuous, mean, na.rm=TRUE)
min <- sapply(continuous, min, na.rm=TRUE)
max <- sapply(continuous, max, na.rm=TRUE)

merged_stats <- cbind(means, min, max)
View(merged_stats)

# Inverse relationship between Times Pumped and Baby Age
correlation <- cor(continuous, use="complete.obs")
View(correlation)

## End Continuous- Continuous variable statisitics


## Start Continuous - Continuous

# Relationship between Amount Baby Eats and Amount Pumped
plot(continuous$Amount_Pumped, continuous$Amount_Baby_Eats , 
     xlab="Amount_Pumped", ylab="Amount_Baby_Eats",pch=19)
abline(reg = lm(continuous$Amount_Baby_Eats ~ continuous$Amount_Pumped),col="blue")

# Slight Relationship between Amount Pumped and Amount of Times Pumped
plot(continuous$Amount_Pumped, continuous$Times_Pumped , 
     xlab="Amount_Pumped", ylab="Times_Pumped",pch=19)
abline(reg = lm(continuous$Times_Pumped ~ continuous$Amount_Pumped),col="blue")

# Relationship between Amount Pumped and Amount of Times Pumped
plot(continuous$Amount_Pumped, continuous$Pumping_Session_Length , 
     xlab="Amount_Pumped", ylab="Pumping_Session_Length",pch=19)
abline(reg = lm(continuous$Pumping_Session_Length ~ continuous$Amount_Pumped),col="blue")


# Not Significant
#
ggplot(continuous, aes(x=Baby_Age)) + geom_density() +
  xlab("Baby Age") + ggtitle("Distribution of Baby Age")

ggplot(continuous, aes(x=Amount_Pumped)) + geom_density() +
  xlab("Amount Pumped") + ggtitle("Distribution of Amount Pumped")

ggplot(continuous, aes(x=Amount_Baby_Eats)) + geom_density() +
  xlab("Amount_Baby_Eats") + ggtitle("Distribution of Amount Baby Eats")

ggplot(continuous, aes(x=Times_Pumped)) + geom_density() +
  xlab("Times_Pumped") + ggtitle("Distribution of Times Pumped")

ggplot(continuous, aes(x=Pumping_Session_Length)) + geom_density() +
  xlab("Pumping_Session_Length") + ggtitle("Distribution of Pumping Session Length")

plot(continuous$Baby_Age, continuous$Amount_Pumped , 
     xlab="Baby_Age", ylab="Amount_Pumped",pch=19)
abline(reg = lm(continuous$Amount_Pumped ~ continuous$Baby_Age),col="blue")

plot(continuous$Baby_Age, continuous$Amount_Baby_Eats , 
     xlab="Baby_Age", ylab="Amount_Baby_Eats",pch=19)
abline(reg = lm(continuous$Amount_Baby_Eats ~ continuous$Baby_Age),col="blue")

# Inverse Relationship between Times Pumped and Amount Baby Eats (*Probably because of age*)
plot(continuous$Amount_Baby_Eats, continuous$Times_Pumped , 
     xlab="Amount_Baby_Eats", ylab="Times_Pumped",pch=19)
abline(reg = lm(continuous$Times_Pumped ~ continuous$Amount_Baby_Eats),col="blue")

#  Relationship between Pumping Session Length and Amount Baby Eats (*Probably because of age*)
plot(continuous$Amount_Baby_Eats, continuous$Pumping_Session_Length , 
     xlab="Amount_Baby_Eats", ylab="Pumping_Session_Length",pch=19)
abline(reg = lm(continuous$Pumping_Session_Length ~ continuous$Amount_Baby_Eats),col="blue")

#  Relationship between Pumping Session Length and Times Pumped (*Probably because of age*)
plot(continuous$Pumping_Session_Length, continuous$Times_Pumped , 
     xlab="Pumping_Session_Length", ylab="Times_Pumped",pch=19)
abline(reg = lm(continuous$Times_Pumped ~ continuous$Pumping_Session_Length),col="blue")

# Times Pumped Declines as Baby Age
plot(continuous$Baby_Age, continuous$Times_Pumped , 
     xlab="Baby_Age", ylab="Times_Pumped",pch=19)
abline(reg = lm(continuous$Times_Pumped ~ continuous$Baby_Age),col="blue")

# Pumping Session Length Increases with Baby Age
plot(continuous$Baby_Age, continuous$Pumping_Session_Length , 
     xlab="Baby_Age", ylab="Pumping_Session_Length",pch=19)
abline(reg = lm(continuous$Pumping_Session_Length ~ continuous$Baby_Age),col="blue")
#
# End Not Significant 
## End Continuous - Continuous



